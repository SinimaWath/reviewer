{
  "name": "tasks-js-ai-reviewer",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Gemini-based AI reviewer for tasks-js coursework",
  "main": "main.ts",
  "scripts": {
    "review": "node main.ts",
    "r:qwen": "AI_PROVIDER=nebius NEBIUS_MODEL='Qwen/Qwen3-Coder-480B-A35B-Instruct' node main.ts",
    "r:kimi": "AI_PROVIDER=nebius NEBIUS_MODEL='moonshotai/Kimi-K2-Thinking' node main.ts",
    "r:g-flash": "AI_MODEL_NAME='gemini-2.5-flash' node main.ts",
    "r:g-pro": "AI_MODEL_NAME='gemini-2.5-pro' node main.ts",
    "r:g3-pro": "AI_MODEL_NAME='gemini-3-pro-preview' node main.ts"
  },
  "dependencies": {
    "@google/genai": "^1.33.0",
    "@google/generative-ai": "^0.11.2",
    "@octokit/rest": "^20.0.2",
    "jsonrepair": "^3.7.0",
    "openai": "^4.76.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1"
  }
}
